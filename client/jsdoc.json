{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_mudules|docs)"
  },
  "opts": {
    "recurse": true,
    "recurseDepth": 5,
    "destination": "docs"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
